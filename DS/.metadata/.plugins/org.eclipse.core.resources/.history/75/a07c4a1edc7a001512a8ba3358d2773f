package controlador;

public class BeanUsuario {
	 
	 private int iD;
	 private String descripcion;
	 private String precio;
	 private String cantidad;
	 private String iVA;
	 private String fechaCaducicad;
	 
	 public int getiD() {
	  return iD;
	 }
	 public void setiD(int id) {
	  this.iD = id;
	 }
	 public String getApellido() {
	  return descripcion;
	 }
	 public void setApellido(String apellido) {
	  this.descripcion = apellido;
	 }
	 public String getEdad() {
	  return precio;
	 }
	 public void setEdad(String edad) {
	  this.precio = edad;
	 }
	 public String getCorreo() {
	  return cantidad;
	 }
	 public void setCorreo(String correo) {
	  this.cantidad = correo;
	 }
	 public String getUsuario() {
	  return iVA;
	 }
	 public void setUsuario(String usuario) {
	  this.iVA = usuario;
	 }
	 public String getClave() {
	  return fechaCaducicad;
	 }
	 public void setClave(String clave) {
	  this.fechaCaducicad = clave;
	 }
	 public BeanUsuario(String nombre, String apellido, String edad,
	   String correo, String usuario, String clave) {
	  super();
	  this.iD = nombre;
	  this.descripcion = apellido;
	  this.precio = edad;
	  this.cantidad = correo;
	  this.iVA = usuario;
	  this.fechaCaducicad = clave;
	 }
	}