                <xsd:documentation>Una cláusula de filtro LDAP para buscar el registro de usuarios de los usuarios.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtro de usuario</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupFilter" use="optional" default="(&amp;(cn=%v)(|(objectclass=groupOfNames)(objectclass=groupOfUniqueNames)))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Cláusula de filtro LDAP para la búsqueda de grupos en el registro de usuarios. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtro de grupo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userIdMap" use="optional" default="inetOrgPerson:uid" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Filtro LDAP que correlaciona el nombre de un usuario con una entrada de LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Correlación de ID de usuario</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupIdMap" use="optional" default="*:cn" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Filtro LDAP que correlaciona el nombre de un grupo con una entrada de LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Correlación de ID de grupo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupMemberIdMap" use="optional" default="groupOfNames:member;groupOfUniqueNames:uniqueMember" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Filtro LDAP que identifica pertenencias de usuarios a grupos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Correlación de ID de miembro de grupo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.netscape-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.registry.ldap.internal.filters.netscape"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Un ID de configuración exclusivo.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.membershipAttribute"> 
        <xsd:annotation> 
            <xsd:documentation>La configuración del atributo de pertenencia LDAP.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Atributo de pertenencia LDAP</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>El nombre del atributo de pertenencia.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nombre del atributo de pertenencia</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="scope" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>El ámbito del atributo de pertenencia.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ámbito</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.membershipAttribute-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.wim.adapter.ldap.membershipAttribute"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Un ID de configuración exclusivo.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.logging"> 
        <xsd:annotation> 
            <xsd:documentation>Controla la captura y la salida de los mensajes de registro y rastreo.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Registro cronológico</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="maxFileSize" use="optional" default="20"> 
            <xsd:annotation> 
                <xsd:documentation>Tamaño máximo de un archivo de registro, en megabytes, antes de que se retrotraiga; un valor 0 significa que no hay ningún límite.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Tamaño máximo del archivo de registro</ext:label> 
                    <ext:variable>com.ibm.ws.logging.max.file.size</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="maxFiles" use="optional" default="2"> 
            <xsd:annotation> 
                <xsd:documentation>Número máximo que se mantendrá de archivos de registro, antes de que se elimine el archivo más antiguo; un valor 0 significa que no hay ningún límite.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Máximo de archivos de registro</ext:label> 
                    <ext:variable>com.ibm.ws.logging.max.files</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="logDirectory" use="optional" default="${server.output.dir}/logs" type="dirLocation"> 
            <xsd:annotation> 
                <xsd:documentation>Ubicación del directorio de los archivos de anotaciones cronológicas. El valor por omisión es ${server.output.dir}/logs.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Directorio de anotaciones cronológicas</ext:label> 
                    <ext:variable>com.ibm.ws.logging.log.directory</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="messageFileName" use="optional" default="messages.log" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nombre del archivo donde se grabará la salida de mensajes relativo con el directorio de registro. El valor por omisión es messages.log.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nombre de archivo de mensaje</ext:label> 
                    <ext:variable>com.ibm.ws.logging.message.file.name</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="traceFileName" use="optional" default="trace.log" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nombre del archivo donde se grabará la salida de rastreo relativo con el directorio de registro. El valor por omisión es trace.log.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nombre de archivo de rastreo</ext:label> 
                    <ext:variable>com.ibm.ws.logging.trace.file.name</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="traceSpecification" use="optional" default="*=info" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Especificación de rastreo que se ajusta a la gramática de especificación de rastreo y especifica el estado inicial de varios componentes de rastreo. Se permite un valor vacío, que se trata como 'inhabilitar todo el rastreo'. Cualquier componente no especificado se inicializará con el estado por omisión *=info.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Especificación de rastreo</ext:label> 
                    <ext:variable>com.ibm.ws.logging.trace.specification</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="hideMessage" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>La lista de mensajes, separados por una coma, que se han configurado para ocultarse de los archivos console.log y message.log. Si los mensajes se han configurado para ocultarse, se redireccionan al archivo trace.log.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mensajes que se van a ocultar</ext:label> 
                    <ext:variable>com.ibm.ws.logging.hideMessage</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="traceFormat" use="optional" default="ENHANCED"> 
            <xsd:annotation> 
                <xsd:documentation>Este formato se utiliza para el registro de rastreo.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Formato de rastreo</ext:label> 
                    <ext:variable>com.ibm.ws.logging.trace.format</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="BASIC"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Se utiliza el formato de rastreo básico.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ENHANCED"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Se utiliza el formato de rastreo básico mejorado.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ADVANCED"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Se utiliza el formato de rastreo avanzado.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="consoleLogLevel" use="optional" default="AUDIT"> 
            <xsd:annotation> 
                <xsd:documentation>El nivel de registro cronológico utilizado para filtrar los mensajes escritos en las corrientes del sistema. El valor por omisión es auditoría.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nivel de registro de consola</ext:label> 
                    <ext:variable>com.ibm.ws.logging.console.log.level</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="INFO"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Los mensajes de información, auditoría y aviso se escribirán en la corriente de salida del sistema. Los mensajes de error se escribirán en la corriente de error del sistema.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="AUDIT"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Los mensajes de auditoría y aviso se escribirán en la corriente de salida del sistema. Los mensajes de error se escribirán en la corriente de error del sistema.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="WARNING"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Los mensajes de aviso se escribirán en la corriente de salida del sistema. Los mensajes de error se escribirán en la corriente de error del sistema.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ERROR"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Los mensajes de error se escribirán en la corriente de error del sistema.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="OFF"> 
                                <xsd:annotation> 
                                    <xsd:documentation>No se escribirá ninguna salida de servidor en las corrientes del sistema.  Sólo se escribirá la salida JVM en las corrientes del sistema.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="copySystemStreams" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Si es true, se grabará System.out en la corriente de salida de sistema y System.err en la salida de errores de sistema. Si el valor es false, System.out y System.err se grabarán en los registros configurados, como messages.log o trace.log, pero no en las corrientes del sistema. El valor por omisión es true.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Copiar System.out y System.err en las corrientes del sistema</ext:label> 
                    <ext:variable>com.ibm.ws.logging.copy.system.streams</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressSensitiveTrace" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>El rastreo del servidor puede exponer datos confidenciales cuando rastrea los datos sin tipo como, por ejemplo, los bytes recibidos a través de una conexión de red. Si es true, impide la exposición de información potencialmente confidencial en los archivos de registro y rastreo. El valor por omisión es false.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Suprimir rastreo de datos confidenciales</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.token.ltpa.LTPAConfiguration"> 
        <xsd:annotation> 
            <xsd:documentation>Configuración de la señal LTPA (Lightweight Third Party Authentication).</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Señal LTPA</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="keysFileName" use="optional" default="${server.output.dir}/resources/security/ltpa.keys" type="fileLocation"> 
            <xsd:annotation> 
                <xsd:documentation>Vía de acceso del archivo que contiene las claves de señal.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Archivo de claves de señal LTPA</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keysPassword" use="optional" default="{xor}CDo9Hgw=" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>Contraseña de las claves de señal. El valor puede almacenarse como texto simple o en formato codificado. Se recomienda codificar la contraseña; utilice la herramienta securityUtility con la opción de codificado.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Contraseña de claves de señal LTPA</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="expiration" use="optional" default="120m" type="minuteDuration"> 
            <xsd:annotation> 
                <xsd:documentation>Cantidad de tiempo, en minutos, después de la cual caduca una señal. Especifica un entero positivo seguido de una unidad de tiempo, que puede ser horas (h) o minutos (m). Por ejemplo, especifique 30 minutos como 30m. Puede incluir varios valores en una sola entrada. Por ejemplo, 1h30m es equivalente a 90 minutos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Caducidad de señal LTPA</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="monitorInterval" use="optional" default="0ms" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>Velocidad con la que el servidor comprueba si hay actualizaciones en el archivo de claves de señal LTPA. Especifica un entero positivo seguido de una unidad de tiempo, que puede ser horas (h), minutos (m), segundos (s) o milisegundos (ms). Por ejemplo, especifique 500 milisegundos como 500ms. Puede incluir varios valores en una sola entrada. Por ejemplo, 1s500ms es equivalente a 1,5 segundos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Velocidad de sondeo del archivo de claves de señal LTPA</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ejbcontainer.runtime"> 
        <xsd:annotation> 
            <xsd:documentation>Define el comportamiento del contenedor EJB. </xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Contenedor EJB</ext:label> 
                <ext:groupDecl id="Advanced"/> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="poolCleanupInterval" use="optional" default="30s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>Intervalo entre las eliminaciones de instancias de bean no utilizadas.  Este valor sólo se aplica a sesiones sin estado y a beans controlados por mensaje. Especifica un entero positivo seguido de una unidad de tiempo, que puede ser horas (h), minutos (m) o segundos (s). Por ejemplo, especifique 30 segundos como 30s. Puede incluir varios valores en una sola entrada. Por ejemplo, 1m30s es equivalente a 90 segundos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Intervalo de limpieza de agrupación</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cacheSize" use="optional" default="2053"> 
            <xsd:annotation> 
                <xsd:documentation>Número de instancias de bean de sesiones con estado que deben almacenarse en memoria caché en la memoria.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Tamaño de memoria caché</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="cacheCleanupInterval" use="optional" default="3s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>Intervalo entre las operaciones de desactivación de instancias de bean de sesiones con estado cuando se sobrepasa el tamaño. Especifica un entero positivo seguido de una unidad de tiempo, que puede ser horas (h), minutos (m) o segundos (s). Por ejemplo, especifique 30 segundos como 30s. Puede incluir varios valores en una sola entrada. Por ejemplo, 1m30s es equivalente a 90 segundos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Intervalo de limpieza de memoria caché</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="startEJBsAtAppStart" use="optional" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica cuándo se inicializarán los tipos de EJB. Si esta propiedad se establece en true, los tipos de EJB se inicializarán en el momento en que las aplicaciones se inicien por primera vez. Si esta propiedad se establece en false, los tipos de EJB se inicializarán en el momento en que una aplicación utilice el tipo de EJB por primera vez. Si no se establece esta propiedad, el comportamiento se determina en función de cada bean, a partir del atributo start-at-app-start del archivo ibm-ejb-jar-ext.xml. Este valor no se aplica ni a los beans controlados por mensaje ni de monoinstancia de arranque. Los beans controlados por mensaje y de monoinstancia de arranque siempre se inicializarán en el momento en que se inicien las aplicaciones.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Iniciar los EJB al inicio de la aplicación</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.monitor.internal.MonitoringFrameworkExtender"> 
        <xsd:annotation> 
            <xsd:documentation>Configuración de la característica de supervisión, que incluye PMI tradicional, FineGrained y las actualizaciones de configuraciones futuras.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Supervisor</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="enableTraditionalPMI" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Propiedad para habilitar o inhabilitar la modalidad de informes PMI tradicional.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Habilitar PMI tradicional</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="filter" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Permite al usuario habilitar/inhabilitar los supervisores basándose en el nombre de grupo como, por ejemplo, WebContainer,JVM,ThreadPool,Session,ConnectionPool, etc.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Grupos filtrados (supervisados)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.group"> 
        <xsd:annotation> 
            <xsd:documentation>Grupo que tiene el rol de seguridad.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Grupo</ext:label> 
